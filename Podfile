# platform :ios, '9.0'

target 'Gotur' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Gotur
  pod 'Alamofire', '~> 4.5'
  pod 'SwiftyJSON'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod 'SwiftSocket'
  # Keyboard Managing
  pod 'IQKeyboardManager'
  # Firebase
  pod 'Firebase/Core'
  pod 'Firebase/Messaging'

  target 'GoturTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GoturUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
